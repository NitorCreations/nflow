{
  "name": "explorer",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@azure/msal-browser": "^3.13.0",
    "@azure/msal-react": "^2.0.15",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "5.16.6",
    "@mui/material": "5.16.6",
    "dagre-d3-es": "7.0.11",
    "date-fns": "3.6.0",
    "lodash": "^4.17.21",
    "moment": "2.30.1",
    "mui-datatables": "4.3.0",
    "mui-rff": "7.4.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-final-form": "^6.5.9",
    "react-router-dom": "6.26.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/http-proxy": "^1.17.9",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.191",
    "@types/mui-datatables": "4.3.12",
    "@types/node": "^18.11.14",
    "@types/q": "^1.5.5",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/typescript-estree": "^5.46.1",
    "@vitejs/plugin-react": "^4.3.1",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "ts-jest": "^29.2.5",
    "typescript": "4.9.5",
    "vite": "5.4.11",
    "vite-tsconfig-paths": "5.1.2"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy-dev": "npm run build && sed -i 's/http:\\/\\/localhost:3000/https:\\/\\/nflow.io/' build/config.js && aws s3 sync build/ s3://nflow-explorer-dev/dev/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "jest-environment-jsdom",
    "moduleNameMapper": {
      "d3": "<rootDir>/node_modules/d3/dist/d3.js",
      "^d3-(.*)$": "<rootDir>/node_modules/d3-$1/dist/d3-$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "modulePaths": [
      "./src"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
