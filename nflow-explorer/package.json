{
  "name": "explorer",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "proxy": "https://nflow.io",
  "dependencies": {
    "@azure/msal-browser": "^3.13.0",
    "@azure/msal-react": "^2.0.15",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "5.16.6",
    "@mui/material": "5.16.6",
    "chokidar": "^3.5.3",
    "d3": "^7.7.0",
    "dagre": "^0.8.5",
    "dagre-d3": "^0.6.4",
    "date-fns": "^2.29.3",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^4.3.8",
    "lint-staged": "^13.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mui-datatables": "4.3.0",
    "mui-rff": "7.4.1",
    "prettier": "^2.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-final-form": "^6.5.9",
    "react-router-dom": "6.26.0",
    "react-scripts": "5.0.1",
    "sass": "^1.56.2",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/d3": "^7.4.0",
    "@types/http-proxy": "^1.17.9",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/mui-datatables": "4.3.12",
    "@types/node": "^18.11.14",
    "@types/q": "^1.5.5",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/typescript-estree": "^5.46.1",
    "typescript": "4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "deploy-dev": "npm run build && sed -i 's/http:\\/\\/localhost:3000/https:\\/\\/nflow.io/' build/config.js && aws s3 sync build/ s3://nflow-explorer-dev/dev/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "d3": "<rootDir>/node_modules/d3/dist/d3.js",
      "^d3-(.*)$": "<rootDir>/node_modules/d3-$1/dist/d3-$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
